{
  "name": "ytheatre",
  "productName": "YTheatre",
  "version": "0.1.4",
  "main": "main.js",
  "homepage": "https://www.ytheatre.app",
  "repository": "https://github.com/EthanHazel/ytheatre",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder -p always",
    "debug-build": "electron-builder"
  },
  "build": {
    "appId": "com.ytheatre.app",
    "productName": "YTheatre",
    "win": {
      "target": "nsis"
    },
    "directories": {
      "output": "build",
      "buildResources": "resources"
    },
    "publish": [
      {
        "provider": "github",
        "repo": "ytheatre",
        "owner": "EthanHazel",
        "releaseType": "release"
      }
    ]
  },
  "author": "Ethan Hazel",
  "license": "ISC",
  "description": "Simple youtube client that uses the TV interface with basic extension support",
  "devDependencies": {
    "electron": "^37.2.2",
    "electron-builder": "^26.0.12"
  },
  "dependencies": {
    "electron-updater": "^6.6.2"
  }
}
